<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.streets.persistence.StoreItem" table="storeitem" schema="" catalog="streets">
        <composite-id mapped="true" class="net.blaklizt.streets.persistence.StoreItemPK">
            <key-property name="storeID">
                <column name="StoreID" sql-type="bigint" length="19" not-null="true"/>
            </key-property>
            <key-property name="itemID">
                <column name="ItemID" sql-type="bigint" length="19" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="cost">
            <column name="Cost" sql-type="double" length="22" not-null="true"/>
        </property>
        <many-to-one name="item" column="ItemID" class="net.blaklizt.streets.persistence.Item" lazy="false" />
    </class>
</hibernate-mapping>