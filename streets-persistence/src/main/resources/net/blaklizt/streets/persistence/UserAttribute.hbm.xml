<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.streets.persistence.UserAttribute" table="userattribute" schema="" catalog="streets">
        <id name="symbiosisUserID" column="UserID" length="10" />
        <property name="gangName" column="GangName" length="30" insert="false" update="false"/>
        <property name="bankBalance" column="BankBalance" length="22" not-null="true"/>
        <property name="healthPoints" column="HealthPoints" length="10" not-null="true"/>
        <property name="locationID" column="LocationID" length="10" not-null="true" insert="false" update="false"/>
        <many-to-one name="location" column="LocationID" class="net.blaklizt.streets.persistence.Location" lazy="false" />
        <many-to-one name="gang" column="GangName" class="net.blaklizt.streets.persistence.Gang" lazy="false" />
    </class>
</hibernate-mapping>