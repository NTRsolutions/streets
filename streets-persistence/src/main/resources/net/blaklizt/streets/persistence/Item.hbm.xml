<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.streets.persistence.Item" table="item" schema="" catalog="streets">
        <id name="itemId" column="ItemID"/>
        <property name="itemName" column="ItemName"/>
        <property name="itemDescription" column="ItemDescription"/>
        <property name="itemValue" column="ItemValue"/>
        <property name="allowMultiple" column="AllowMultiple"/>
    </class>
</hibernate-mapping>