<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="net.blaklizt.streets.persistence.Location" table="location" schema="" catalog="streets">
        <id name="locationId">
            <column name="LocationID" sql-type="bigint" length="19" not-null="true"/>
        </id>
        <property name="locationName">
            <column name="LocationName" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="northLocationId">
            <column name="NorthLocationID" sql-type="bigint" length="19"/>
        </property>
        <property name="southLocationId">
            <column name="SouthLocationID" sql-type="bigint" length="19"/>
        </property>
        <property name="eastLocationId">
            <column name="EastLocationID" sql-type="bigint" length="19"/>
        </property>
        <property name="westLocationId">
            <column name="WestLocationID" sql-type="bigint" length="19"/>
        </property>
        <property name="controllingGangName" insert="false" update="false">
            <column name="ControllingGangName" sql-type="varchar" length="30"/>
        </property>
        <property name="bestBusinessType">
            <column name="BestBusinessType" sql-type="varchar" length="30"/>
        </property>
        <property name="currentBusinessType" insert="false" update="false">
            <column name="CurrentBusinessType" sql-type="varchar" length="30"/>
        </property>
        <property name="businessProblemID">
            <column name="BusinessProblemID" sql-type="bigint" length="19"/>
        </property>
        <many-to-one name="controllingGang" column="ControllingGangName" class="net.blaklizt.streets.persistence.Gang" lazy="false" />
        <many-to-one name="currentBusiness" column="CurrentBusinessType" class="net.blaklizt.streets.persistence.Business" lazy="false" />
    </class>
</hibernate-mapping>